package recursion;
import java.util.*;

public class PrintIncreasing {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		Scanner s = new Scanner(System.in);
		int no = s.nextInt();
		
		PrintIncreasing(no);
		
	}
	
	public static void PrintIncreasing(int n) {
		
		if(n==0) {
			return ;
		}
		
		PrintIncreasing(n-1);
		System.out.println(n);
	}

}
